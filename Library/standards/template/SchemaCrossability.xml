	<!-- template::crossability:species -->
	<META>
		<NODE tag=":taxon:cross:species"
			  pid="template::crossability:species">
			<item const="kTAG_ID_SYMBOL">LISTSPCROSS</item>
			<item const="kTAG_NODE_TYPE">
				<item>:type:node:template</item>
				<item>:kind:property-node</item>
			</item>
			<item const="kTAG_TOKEN">;</item>
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[List of species crosses]]></item>
				</item>
			</item>
			<item const="kTAG_DESCRIPTION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[List the species that have been crossed, separated the items by semicolons (;) without space.]]></item>
				</item>
			</item>
		</NODE>
		<EDGE>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
			<item const="kTAG_OBJECT" node="pid">schema::template:crossability</item>
		</EDGE>
	</META>
		
