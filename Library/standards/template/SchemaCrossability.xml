				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[Method(s) used for crosses]]></item>
				</item>
			</item>
			<item const="kTAG_DESCRIPTION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[Describe the method(s) used for each identified crosses. Multiple values are separated by semicolon (;) without space.]]></item>
				</item>
			</item>
		</NODE>
		<EDGE>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
			<item const="kTAG_OBJECT" node="pid">schema::template:crossability</item>
		</EDGE>
