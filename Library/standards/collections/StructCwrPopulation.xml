		<!-- schema::location:site -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="pid">schema::location:site</item>
			<item const="kTAG_PREDICATE">:predicate:SUBCLASS-OF</item>
			<item const="kTAG_OBJECT" node="tag">cwr:population</item>
		</EDGE>
