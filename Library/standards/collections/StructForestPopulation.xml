<?xml version="1.0" encoding="UTF-8"?>
<!--
	FOREST GENE CONSERVATION UNITS POPULATION STRUCTURE
	StructForestPopulation.xml
-->
<METADATA
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="https://gist.githubusercontent.com/milko/f2dcea3c3a94f69bb0bb/raw/715d8fd15a66ad25466c9efc4f767fd31fc9a21a/Dictionary.xsd">
	
	<!-- Population structure -->
	
	<!-- fcu:population -->
	<META>
		<!-- schema::taxon -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="pid">schema::taxon</item>
			<item const="kTAG_PREDICATE">:predicate:SUBCLASS-OF</item>
			<item const="kTAG_OBJECT" node="tag">fcu:population</item>
		</EDGE>
		
		<!-- :taxon:distribution -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:taxon:distribution</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
			<item const="kTAG_OBJECT" node="tag">fcu:population</item>
		</EDGE>
		
		<!-- schema:fcu:population -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="pid">schema:fcu:population</item>
			<item const="kTAG_PREDICATE">:predicate:SUBCLASS-OF</item>
			<item const="kTAG_OBJECT" node="tag">fcu:population</item>
		</EDGE>
	</META>
</METADATA>
