<?xml version="1.0" encoding="UTF-8"?>
<!--
	TERM STRUCTURE
	StructureTerm.xml
-->
<METADATA
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="https://gist.githubusercontent.com/milko/f2dcea3c3a94f69bb0bb/raw/715d8fd15a66ad25466c9efc4f767fd31fc9a21a/Dictionary.xsd">
	
	<!-- TERM -->
	
	<!-- struct::domain:term -->
	<META>
		<NODE term=":domain:term" pid="struct::domain:term">
			<item const="kTAG_NODE_TYPE">
				<item>:kind:root-node</item>
				<item>:type:node:struct</item>
			</item>
			<item const="kTAG_DESCRIPTION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[This structure contains all default properties of term objects.]]></item>
				</item>
			</item>
		</NODE>
		
		<!-- PROPERTIES -->
	
		<!-- kTAG_NAMESPACE -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:namespace</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- kTAG_ID_LOCAL -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:id-local</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- kTAG_ID_GRAPH -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:id-graph</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- kTAG_LABEL -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:label</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- kTAG_DEFINITION -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:definition</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- kTAG_SYNONYM -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:synonym</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- kTAG_TERM_TYPE -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:type:term</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- kTAG_MASTER -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:max-val</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- kTAG_PATTERN -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:master</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
		
		<!-- RECORD PROPERTIES -->
	
		<!-- schema::record -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="pid">schema::record</item>
			<item const="kTAG_PREDICATE">:predicate:SUBCLASS-OF</item>
		</EDGE>
	</META>
</METADATA>
