<?xml version="1.0" encoding="UTF-8"?>
<!--
	RECORD SCHEMA
	SchemaRecord.xml
-->
<METADATA
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="https://gist.githubusercontent.com/milko/f2dcea3c3a94f69bb0bb/raw/715d8fd15a66ad25466c9efc4f767fd31fc9a21a/Dictionary.xsd">
	
	<!-- SCHEMA -->
	
	<!-- :record -->
	<META>
		<NODE term=":record" pid="schema::record">
			<item const="kTAG_NODE_TYPE">
				<item>:type:node:schema</item>
			</item>
			<item const="kTAG_DESCRIPTION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[This schema contains all default properties which provide record specific information.]]></item>
				</item>
			</item>
		</NODE>
	
		<!-- PROPERTIES -->
	
		<!-- kTAG_RECORD_CREATED -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:record:created</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- kTAG_RECORD_MODIFIED -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:record:modified</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	</META>
</METADATA>
