	<!-- NODE -->
	
	<!-- struct::domain:node -->
	<META>
		<NODE term=":domain:node" pid="struct::domain:node">
			<item const="kTAG_NODE_TYPE">
				<item>:kind:root-node</item>
				<item>:type:node:struct</item>
			</item>
			<item const="kTAG_DESCRIPTION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[This structure contains all default properties of node objects.]]></item>
				</item>
			</item>
		</NODE>
		
		<!-- PROPERTIES -->
	
		<!-- kTAG_ID_PERSISTENT -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:id-persistent</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- kTAG_ID_GRAPH -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:id-graph</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- kTAG_TERM -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:term</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- kTAG_TAG -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:tag</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- kTAG_NODE_TYPE -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:type:node</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- kTAG_MASTER -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:master</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
		
		<!-- RECORD PROPERTIES -->
	
		<!-- schema::ref-count -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="pid">schema::ref-count</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- schema::record -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="pid">schema::record</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	</META>
