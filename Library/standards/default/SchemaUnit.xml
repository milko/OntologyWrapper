<?xml version="1.0" encoding="UTF-8"?>
<!--
	UNIT IDENTIFICATION SCHEMA
	SchemaUnit.xml
-->
<METADATA
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="https://gist.githubusercontent.com/milko/f2dcea3c3a94f69bb0bb/raw/715d8fd15a66ad25466c9efc4f767fd31fc9a21a/Dictionary.xsd">
	
	<!-- SCHEMA -->
	
	<!-- schema::domain:unit -->
	<META>
		<NODE term=":domain:unit" pid="schema::domain:unit">
			<item const="kTAG_NODE_TYPE">
				<item>:type:node:schema</item>
			</item>
			<item const="kTAG_DESCRIPTION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[This schema contains all default properties which are used to identify objects derived from the unit class.]]></item>
				</item>
			</item>
		</NODE>
	
		<!-- PROPERTIES -->
	
		<!-- kTAG_DOMAIN -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:unit:domain</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
		<EDGE>
			<item const="kTAG_SUBJECT" node="pid">enum::domain</item>
			<item const="kTAG_PREDICATE">:predicate:TYPE-OF</item>
			<item const="kTAG_OBJECT" node="tag">:unit:domain</item>
		</EDGE>
	
		<!-- kTAG_AUTHORITY -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:unit:authority</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- kTAG_COLLECTION -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:unit:collection</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- kTAG_IDENTIFIER -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:unit:identifier</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- kTAG_VERSION -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:unit:version</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	</META>
</METADATA>
