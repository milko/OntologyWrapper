<?xml version="1.0" encoding="UTF-8"?>
<!--
	ERROR SCHEMA
	SchemaError.xml
-->
<METADATA
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="https://gist.githubusercontent.com/milko/f2dcea3c3a94f69bb0bb/raw/715d8fd15a66ad25466c9efc4f767fd31fc9a21a/Dictionary.xsd">
	
	<!-- SCHEMA -->
	
	<!-- schema::error -->
	<META>
		<NODE term=":error" pid="schema::error">
			<item const="kTAG_NODE_TYPE">
				<item>:type:node:schema</item>
			</item>
			<item const="kTAG_DESCRIPTION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[This schema contains all default properties regarding an error.]]></item>
				</item>
			</item>
		</NODE>
	
		<!-- PROPERTIES -->
	
		<!-- kTAG_ERROR_TYPE -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:error:type</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- kTAG_ERROR_CODE -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:error:code</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	
		<!-- kTAG_ERROR_RESOURCE -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:error:resource</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
	</META>
</METADATA>
