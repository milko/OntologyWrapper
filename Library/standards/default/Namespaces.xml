<?xml version="1.0" encoding="UTF-8"?>
<!--
	DEFAULT NAMESPACES
	Namespaces.xml
-->
<METADATA
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://resources.grinfo.net/Schema/Dictionary.xsd">
	
	<!-- Namespaces -->
	
	<META>
		<!-- Default namespace -->
		<TERM lid="">
			<item const="kTAG_LABEL">
				<item lang="en">Default namespace</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item lang="en"><![CDATA[This <em>namespace</em> groups all default or built-in terms of the ontology, these are the elements that will be used to build the ontology itself.]]></item>
			</item>
		</TERM>
		
		<!-- Domain -->
		<TERM ns="" lid="domain">
			<item const="kTAG_LABEL">
				<item lang="en">Domain</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item lang="en"><![CDATA[The <em>domain</em> represents the <em>kind</em> or <em>class</em>, it indicates <em>what</em> an object is.]]></item>
			</item>
		</TERM>
		
		<!-- Type -->
		<TERM ns="" lid="type">
			<item const="kTAG_LABEL">
				<item lang="en">Type</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item lang="en"><![CDATA[The <em>type</em> describes the <em>nature or composition</em> of a <em>data value</em>.]]></item>
			</item>
		</TERM>
		
		<!-- Relationship -->
		<TERM ns="" lid="relationship">
			<item const="kTAG_LABEL">
				<item lang="en">Relationship</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item lang="en"><![CDATA[A <em>relationship</em> represents the <em>term</em> of a <em>directed graph relation</em>.]]></item>
			</item>
		</TERM>
		
		<!-- Reference -->
		<TERM ns=":type" lid="ref">
			<item const="kTAG_LABEL">
				<item lang="en">Reference</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item lang="en"><![CDATA[A <em>reference</em> represents a <em>link</em> to an object.]]></item>
			</item>
		</TERM>
		
		<!-- Connection -->
		<TERM ns="" lid="connection">
			<item const="kTAG_LABEL">
				<item lang="en">Connection</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item lang="en"><![CDATA[A <em>connection</em> groups <em>network connection related attributes</em>.]]></item>
			</item>
		</TERM>
	</META>
</METADATA>
