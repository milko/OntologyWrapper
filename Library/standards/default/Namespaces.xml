<?xml version="1.0" encoding="UTF-8"?>
<!--
	DEFAULT NAMESPACES
	Namespaces.xml
-->
<METADATA
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://resources.grinfo.net/Schema/Dictionary.xsd">
	
	<!-- Namespaces -->
	
	<META>
		<!-- Default namespace -->
		<TERM lid="">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Default namespace</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[This <em>namespace</em> groups all default or built-in terms of the ontology, these are the elements that will be used to build the ontology itself.]]></item>
				</item>
			</item>
		</TERM>
		
		<!-- Domain -->
		<TERM ns="" lid="domain">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Domain</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[The <em>domain</em> represents the <em>kind</em> or <em>class</em>, it indicates <em>what</em> an object is.]]></item>
				</item>
			</item>
		</TERM>
		
		<!-- Type -->
		<TERM ns="" lid="type">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Type</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[The <em>type</em> describes the <em>nature</em> or <em>composition</em> of an object.]]></item>
				</item>
			</item>
		</TERM>
		
		<!-- Kind -->
		<TERM ns="" lid="kind">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Kind</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[The <em>kind</em> describes the <em>function</em> or <em>context</em> of an object.]]></item>
				</item>
			</item>
		</TERM>
		
		<!-- Offset -->
		<TERM ns="" lid="offset">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Offset</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[The <em>offset</em> represents a <em>path to a leaf offset</em>.]]></item>
				</item>
			</item>
		</TERM>
		
		<!-- Predicate -->
		<TERM ns="" lid="predicate">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Predicate</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[A <em>predicate</em> qualifies a <em>directed graph relationship</em>.]]></item>
				</item>
			</item>
		</TERM>
		
		<!-- Relationship -->
		<TERM ns="" lid="relationship">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Relationship</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[A <em>relationship</em> represents the <em>term</em> of a <em>directed graph relation</em>.]]></item>
				</item>
			</item>
		</TERM>
		
		<!-- Reference -->
		<TERM ns=":type" lid="ref">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Reference</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[A <em>reference</em> represents a <em>link</em> to an object.]]></item>
				</item>
			</item>
		</TERM>
		
		<!-- Reference count -->
		<TERM ns="" lid="ref-count">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Reference count</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[A <em>reference count</em> keeps track of <em>how many objects reference the current object</em>.]]></item>
				</item>
			</item>
		</TERM>
		
		<!-- Connection -->
		<TERM ns="" lid="connection">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Connection</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[A <em>connection</em> groups <em>network connection related attributes</em>.]]></item>
				</item>
			</item>
		</TERM>
		
		<!-- Entity -->
		<TERM ns="" lid="entity">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Entity</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[An <em>entity</em> groups <em>organisation, institution or person related attributes</em>.]]></item>
				</item>
			</item>
		</TERM>
		
		<!-- Unit -->
		<TERM ns="" lid="unit">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Unit</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[A <em>unit</em> is an <em>object</em> that can be considered an <em>independant entity</em> and not a part or component of another object.]]></item>
				</item>
			</item>
		</TERM>
	</META>
</METADATA>
