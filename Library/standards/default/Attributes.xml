	<!-- Internal attributes -->
	
	<META>
		<!-- kTAG_NID -->
		<TERM lid="_id">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Native identifier</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[The <em>native identifier</em> represents the <em>primary key</em> of an object, all objects which are persistent must have this property.]]></item>
				</item>
			</item>
		</TERM>
		
		<!-- kTAG_CLASS -->
		<TERM lid="_class">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Class</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[The <em>class</em> represents the <em>object class name</em>, this <em>string</em> is used to instantiate the correct object once loaded from a container.]]></item>
				</item>
			</item>
		</TERM>
		
		<!-- kTAG_SHAPE_TYPE -->
		<TERM lid="type">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Shape type</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[This offset represents the <em>type of a shape</em>, this string is used to indicate  what type of object the shape represents. This offset is followed by the <tt>coordinates</tt> offset that contains the shape geometry.]]></item>
				</item>
			</item>
		</TERM>
		
		<!-- kTAG_SHAPE_GEOMETRY -->
		<TERM lid="coordinates">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Shape geometry</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[This offset represents the <em>geometry of a shape</em>, this string labels the coordinates lists that comprise a shape geometry. This offset is preceded by the <tt>type</tt> offset that contains the shape type.]]></item>
				</item>
			</item>
		</TERM>
	</META>
	
	<!-- Identification attributes -->
	
	<META>
		<!-- kTAG_NAMESPACE -->
		<TERM ns="" lid="namespace">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Namespace</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[The <em>namespace</em> is used to <em>disambiguate homonym identifiers</em>.]]></item>
				</item>
			</item>
		</TERM>
	
		<!-- kTAG_ID_LOCAL -->
		<TERM ns="" lid="id-local">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Local identifier</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[A <em>local identifier</em> is used to uniquely identify objects <em>within a namespace</em>.]]></item>
				</item>
			</item>
		</TERM>
	
		<!-- kTAG_ID_PERSISTENT -->
		<TERM ns="" lid="id-persistent">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Persistent identifier</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[A <em>persistent identifier</em> is <em>immutable</em> and represents a <em>global identifier</em>.]]></item>
				</item>
			</item>
		</TERM>
	
		<!-- kTAG_ID_VALID -->
		<TERM ns="" lid="id-valid">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Valid identifier</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[The <em>valid identifier</em> is a <em>persistent identifier</em> which is <em>valid</em> or <em>official</em>, as opposed to a persistent identifier which is invalid or obsolete.]]></item>
				</item>
			</item>
		</TERM>
	
		<!-- kTAG_ID_SEQUENCE -->
		<TERM ns="" lid="id-sequence">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Sequence number</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[A <em>sequence number</em> is an <em>identifier</em> which is <em>unique across objects belonging to the same collection</em>, this identifier is <em>serial</em>, in that it is <em>assigned at commit time</em> and subsequently <em>incremented</em>.]]></item>
				</item>
			</item>
		</TERM>
	
		<!-- kTAG_ID_GRAPH -->
		<TERM ns="" lid="id-graph">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Graph reference</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[A <em>graph reference</em> is an <em>identifier</em> which <em>relates the current object with an element of a graph or ontology</em>.]]></item>
				</item>
			</item>
		</TERM>
	</META>
	
