<?xml version="1.0" encoding="UTF-8"?>
<!--
	DEFAULT ENTITY KINDS
	EntityKinds.xml
-->
<METADATA
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://resources.grinfo.net/Schema/Dictionary.xsd">
	
	<!-- Entity kind -->
	
	<!-- :kind:entity -->
	<META>
		<NODE term=":kind:entity">
			<item const="kTAG_NODE_TYPE">
				<item>:type:node:type</item>
				<item>:kind:enumerated-node</item>
			</item>
			<item const="kTAG_DESCRIPTION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[This type defines the enumerated set of all entity kinds.]]></item>
				</item>
			</item>
		</NODE>
	</META>
	
	<!-- Entity kind enumerations -->
	
	<!-- :kind:entity:100 -->
	<META>
		<TERM ns=":kind:entity" lid="100">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Plant genetic resources</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[The entity operates with plant genetic resources.]]></item>
				</item>
			</item>
		</TERM>
		<NODE>
			<item const="kTAG_NODE_TYPE">
				<item>:type:node:enumeration</item>
			</item>
		</NODE>
		<EDGE>
			<item const="kTAG_PREDICATE">:relationship:predicate:ENUM-OF</item>
			<item const="kTAG_OBJECT" node="term">:kind:entity</item>
		</EDGE>
	</META>

	<!-- :kind:entity:200 -->
	<META>
		<TERM ns=":kind:entity" lid="200">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Collection</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[The entity maintains plant genetic resources collections.]]></item>
				</item>
			</item>
		</TERM>
		<NODE>
			<item const="kTAG_NODE_TYPE">
				<item>:type:node:enumeration</item>
			</item>
		</NODE>
		<EDGE>
			<item const="kTAG_PREDICATE">:relationship:predicate:ENUM-OF</item>
			<item const="kTAG_OBJECT" node="term">:kind:entity</item>
		</EDGE>
	</META>
</METADATA>
