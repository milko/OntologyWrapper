				<item>:kind:root-node</item>
				<item>:type:node:struct</item>
			</item>
			<item const="kTAG_DESCRIPTION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[This structure contains all default properties of edge objects.]]></item>
				</item>
			</item>
		</NODE>
		
		<!-- kTAG_ID_GRAPH -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:id-graph</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
		
		<!-- kTAG_SUBJECT -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:relationship:subject</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
		
		<!-- kTAG_GRAPH_SUBJECT -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:relationship:graph-subject</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
		
		<!-- kTAG_PREDICATE -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:predicate</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
		
		<!-- kTAG_OBJECT -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="tag">:relationship:object</item>
