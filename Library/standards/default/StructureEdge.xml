			<item const="kTAG_SUBJECT" node="tag">:relationship:graph-object</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
		</EDGE>
		
		<!-- RECORD PROPERTIES -->
	
		<!-- schema::ref-count -->
		<EDGE>
			<item const="kTAG_SUBJECT" node="pid">schema::ref-count</item>
			<item const="kTAG_PREDICATE">:predicate:PROPERTY-OF</item>
