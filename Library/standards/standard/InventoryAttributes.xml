<?xml version="1.0" encoding="UTF-8"?>
<!--
	INVENTORY ATTRIBUTES
	InventoryAttributes.xml
-->
<METADATA
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://resources.grinfo.net/Schema/Dictionary.xsd">
	
	<!-- Inventory attributes -->
	
	<!-- :inventory:dataset -->
	<META>
		<TERM ns=":inventory" lid="dataset">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Dataset</item>
				</item>
			</item>
			<item const="kTAG_DEFINITION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[Source <em>dataset</em> or <em>catalogue</em>.]]></item>
				</item>
			</item>
		</TERM>
		<TAG>
			<item const="kTAG_TERMS">
				<item>:inventory:dataset</item>
			</item>
			<item const="kTAG_DATA_TYPE">:type:string</item>
			<item const="kTAG_DATA_KIND">
				<item>:type:categorical</item>
			</item>
			<item const="kTAG_DESCRIPTION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[Name or code identifying the dataset or catalogue that provided the information.]]></item>
				</item>
			</item>
		</TAG>
		<NODE tag=":inventory:dataset">
			<item const="kTAG_NODE_TYPE">
				<item>:kind:property-node</item>
			</item>
		</NODE>
	</META>
	
	<!-- :inventory:NICODE -->
	<META>
		<TERM ns=":inventory" lid="NICODE">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">National inventory</item>
				</item>
			</item>
		</TERM>
		<TAG>
			<item const="kTAG_TERMS">
				<item>:inventory:NICODE</item>
			</item>
			<item const="kTAG_DATA_TYPE">:type:string</item>
			<item const="kTAG_DATA_KIND">
				<item>:type:categorical</item>
			</item>
			<item const="kTAG_DESCRIPTION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[Code identifying the National Inventory. In general the code of the country preparing the National Inventory.]]></item>
				</item>
			</item>
		</TAG>
		<NODE tag=":inventory:NICODE">
			<item const="kTAG_NODE_TYPE">
				<item>:kind:property-node</item>
			</item>
		</NODE>
	</META>
	
	<!-- :inventory:INSTCODE -->
	<META>
		<TERM ns=":inventory" lid="INSTCODE">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT">Institute</item>
				</item>
			</item>
		</TERM>
		<TAG>
			<item const="kTAG_TERMS">
				<item>:inventory:INSTCODE</item>
			</item>
			<item const="kTAG_DATA_TYPE">:type:ref:unit</item>
			<item const="kTAG_DESCRIPTION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[This attribute holds the <em>FAO code</em> of the responsible institute.]]></item>
				</item>
			</item>
		</TAG>
		<NODE tag=":inventory:INSTCODE">
			<item const="kTAG_NODE_TYPE">
				<item>:kind:property-node</item>
			</item>
		</NODE>
	</META>
</METADATA>
