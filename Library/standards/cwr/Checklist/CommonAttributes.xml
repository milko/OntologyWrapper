<METADATA
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="https://gist.githubusercontent.com/milko/f2dcea3c3a94f69bb0bb/raw/715d8fd15a66ad25466c9efc4f767fd31fc9a21a/Dictionary.xsd">
	
	<!-- Crop wild relative attributes -->
	
	<!-- cwr:REF -->
	<META>
		<TERM ns="cwr" lid="REF">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[References]]></item>
				</item>
			</item>
		</TERM>
		<TAG>
			<item const="kTAG_TERMS">
				<item>cwr:REF</item>
			</item>
			<item const="kTAG_DATA_TYPE">:type:string</item>
			<item const="kTAG_DATA_KIND">
				<item>:type:discrete</item>
				<item>:type:list</item>
			</item>
			<item const="kTAG_SYNONYM">
				<item>REF</item>
			</item>
			<item const="kTAG_DESCRIPTION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[Any bibliographic references related to the publication, the format should include the author, date, title, journal, volume and page number or the DOI number.]]></item>
				</item>
			</item>
		</TAG>
		<NODE>
			<item const="kTAG_NODE_TYPE">
				<item>:kind:property-node</item>
			</item>
		</NODE>
	</META>
	
	<!-- cwr:URL -->
	<META>
		<TERM ns="cwr" lid="URL">
			<item const="kTAG_LABEL">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[URL links]]></item>
				</item>
			</item>
		</TERM>
		<TAG>
			<item const="kTAG_TERMS">
				<item>cwr:URL</item>
			</item>
			<item const="kTAG_DATA_TYPE">:type:url</item>
			<item const="kTAG_DATA_KIND">
				<item>:type:discrete</item>
			</item>
			<item const="kTAG_SYNONYM">
				<item>URL</item>
			</item>
			<item const="kTAG_DESCRIPTION">
				<item>
					<item const="kTAG_LANGUAGE">en</item>
					<item const="kTAG_TEXT"><![CDATA[URL linking to any additional data about the species either in the institute or from another source.]]></item>
				</item>
			</item>
		</TAG>
		<NODE>
			<item const="kTAG_NODE_TYPE">
				<item>:kind:property-node</item>
			</item>
		</NODE>
